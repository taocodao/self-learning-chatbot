{
    "version": 2,
    "builds": [
        {
            "src": "packages/backend/src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "packages/backend/src/server.ts"
        }
    ],
    "outputDirectory": "packages/backend/dist"
}